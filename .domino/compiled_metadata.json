{
    "MyNewPiece": {
        "name": "MyNewPiece",
        "tags": [
            "NewPiece"
        ],
        "style": {
            "node_label": "Simple Piece",
            "node_type": "default",
            "node_style": {
                "backgroundColor": "#ebebeb"
            },
            "useIcon": true,
            "icon_class_name": "fas fa-database",
            "iconStyle": {
                "cursor": "pointer"
            }
        },
        "description": "This is an example of a simple Domino Piece",
        "container_resources": {
            "requests": {
                "cpu": "100",
                "memory": "128"
            },
            "limits": {
                "cpu": "500",
                "memory": "512"
            }
        },
        "input_schema": {
            "title": "InputModel",
            "type": "object",
            "properties": {
                "in_argument_1": {
					"title": "Argument 1",
                    "description": "an argument of numeric type with default value",
                    "type": "number",
					"default": 1.0
                },
                "in_argument_2": {
                    "title": "Argument 2",
                    "description": "a required argument of string type",
                    "type": "string"
                },
                "in_argument_3": {
                    "title": "Argument 3",
                    "description": "a required argument of boolean type",
                    "type": "boolean"
                }
            },
            "required": [
				"in_argument_2",
				"in_argument_3"
			]
        },
        "output_schema": {
            "title": "OutputModel",
            "type": "object",
            "properties": {
                "out_argument_1": {
                    "title": "Argument 1",
                    "description": "an argument of string type",
                    "type": "string"
                },
                "out_file_path": {
                    "title": "File path",
                    "description": "The path to a file saved in a shared storage",
                    "type": "string"
                }
            },
            "required": []
        },
        "secrets_schema": {
            "title": "SecretsModel",
			"type": "object"
			"description": "New Piece secret model", 
            "properties": {
                "SECRET_VAR": {
                    "title": "Example Secret variable",
                    "description": "Secret variable",
                    "type": "string"
                }
            },
            "required": [
				"SECRET_VAR"
			]
        }
    }
}